{
    "manifestVersion": 1,
    "id": "myappci-upload-task",
    "name": "MyAppCI build tools",
    "version": "0.0.78",
    "publisher": "MyAppCI",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tools to publish apps into MyAppCI CI branches.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/myappci.png"        
    },
    "files": [
        {
            "path": "publishTask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "publishTask"
            }
        }
    ]
}
 